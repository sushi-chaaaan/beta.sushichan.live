{
  "name": "sushichan.live",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist",
    "astro": "astro",
    "prepare": "husky install",
    "eslint": "eslint ./src/**/*.{js,ts,jsx,tsx,astro} --ignore-path .gitignore",
    "eslint:fix": "eslint --fix ./src/**/*.{js,ts,jsx,tsx,astro} --ignore-path .gitignore",
    "eslint:precommit": "eslint --max-warnings 0 --ignore-path .gitignore",
    "fmt": "prettier --write . --plugin=prettier-plugin-astro",
    "fmt:precommit": "prettier --check --plugin=prettier-plugin-astro",
    "stylelint": "stylelint \"./src/**/*.{css,scss,astro}\"",
    "stylelint:fix": "stylelint --fix \"./src/**/*.{css,scss,astro}\"",
    "stylelint:precommit": "stylelint --max-warnings 0 --ignore-path .gitignore",
    "markuplint": "markuplint \"./**/*.{jsx,tsx,astro}\"",
    "markuplint:precommit": "markuplint --no-color --problem-only \"./**/*.{jsx,tsx,astro}\"",
    "ncu": "ncu -u --target minor",
    "ncu:latest": "ncu --target latest"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "npm run eslint:precommit",
      "npm run fmt:precommit"
    ],
    "./**/*.{jsx,tsx}": [
      "npm run eslint:precommit",
      "npm run fmt:precommit",
      "npm run markuplint:precommit"
    ],
    "./**/*.{css,scss}": "npm run stylelint:precommit",
    "./**/*.astro": [
      "npm run eslint:precommit",
      "npm run fmt:precommit",
      "npm run markuplint:precommit",
      "npm run stylelint:precommit"
    ]
  },
  "dependencies": {
    "@astrojs/cloudflare": "^7.6.3",
    "@astrojs/mdx": "^1.1.3",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/react": "^3.0.4",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/tailwind": "^5.0.2",
    "@iconify/iconify": "^3.1.1",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@resvg/resvg-js": "^2.6.0",
    "@unocss/reset": "^0.57.1",
    "astro": "^3.4.0",
    "astro-auto-import": "^0.3.1",
    "astro-seo": "^0.8.0",
    "budoux": "^0.5.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fetch-site-metadata": "^0.1.3",
    "ie-buster": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.13.0",
    "react-tweet": "^3.1.1",
    "rehype-katex": "^7.0.0",
    "remark-emoji": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-unwrap-images": "^4.0.0",
    "sanitize-html": "^2.11.0",
    "satori": "^0.10.9",
    "tailwind-variants": "^0.1.18",
    "tailwindcss": "^3.3.5",
    "wrangler": "^3.15.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@iconify/json": "^2.2.134",
    "@markuplint/astro-parser": "^3.11.0",
    "@markuplint/jsx-parser": "^3.11.0",
    "@markuplint/react-spec": "^3.12.0",
    "@textlint/textlint-plugin-markdown": "^13.3.3",
    "@types/eslint": "8.44.6",
    "@types/node": "20.8.9",
    "@types/prettier": "^3",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/sanitize-html": "^2.9.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "astro-eslint-parser": "^0.16.0",
    "daisyui": "latest",
    "eslint": "^8.52.0",
    "eslint-config-astro": "^1.0.8",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-perfectionist": "^2.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "markuplint": "^3.14.0",
    "npm-check-updates": "^16.14.6",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-value-no-unknown-custom-properties": "^4.0.0",
    "textlint": "^13.3.3",
    "textlint-rule-preset-japanese": "^8.0.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "18.18.0",
    "yarn": "3.6.4"
  }
}
